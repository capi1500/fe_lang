fn nice_print(printer: fn() -> string) {
    print_string("Very nice printing");
    print_string(printer());
}

fn main() {
    for (x in (1..10)) {
        nice_print(|&x|(a: i32) -> string { // borrow x
            ((x + 6 * 4 - 3) % 6) as string
        }); // Should print (x + (6 * 4) - 3) % 6
    }
}