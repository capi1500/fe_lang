fn nice_print(printer: Fn() -> i32) {
    print_string("Very nice printing\n");
    print_i32(printer());
    print_string("\n");
}

fn main() {
    var i = 1;
	while (i < 10) {
        nice_print(|&i|(a: i32) -> i32 { // borrow x
            ((i + 6 * 4 - 3) % 6)
        }); // Should print (x + (6 * 4) - 3) % 6
		i += 1;
    }
}