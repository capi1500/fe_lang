fn sort(arr: &mut [i32]) {
	const n = length_i32(arr);
	for (i in 1..n - 1) {
		var j = i;
		while (j > 0 && arr[j - 1] > arr[j]) {
			const tmp = arr[j - 1];
			arr[j - 1] = arr[j];
			arr[j] = tmp;
			j -= 1;
			print(arr);
		}
	}
}

fn print(arr: &[i32]) {
	print_char('[');
	const n = length_i32(arr);
	for (i in 0..(n - 1)) {
		print_i32(arr[i]);
		if (i + 1 != n) {
			print_string(", ");
		}
	}
	print_string("]\n");
}

fn input_arr() -> [i32] {
	[input_i32(); input_i32()]
}

fn main() {
	var arr = input_arr();
	print(&arr);
	sort(&mut arr);
	print(&arr);
}